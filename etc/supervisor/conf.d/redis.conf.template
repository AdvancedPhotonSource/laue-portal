[program:redis]
command={{CONDA_BASE}}/envs/{{CONDA_ENV}}/bin/redis-server {{SCRIPT_DIR}}/etc/redis/redis.conf
directory={{SCRIPT_DIR}}
autostart=true
autorestart=true
stdout_logfile={{SUPERVISOR_DIR}}/logs/redis.log
stderr_logfile={{SUPERVISOR_DIR}}/logs/redis_err.log
stdout_logfile_maxbytes=10MB
stdout_logfile_backups=5
stderr_logfile_maxbytes=10MB
stderr_logfile_backups=5
priority=100
